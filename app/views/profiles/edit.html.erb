<%= form_for (@profile) do |f| %>
        <div>
        <%= f.label :first_name %>
        <%= f.text_field :first_name %>
      </div>
      <div>
        <%= f.label :last_name %>
        <%= f.text_field :last_name %>
      </div>
      <div>
        <%= f.label :bio %>
        <%= f.text_area :bio %>
      </div>
      <%= f.submit "Create Profile" %>
<% end %>